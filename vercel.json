{
  "installCommand": "yarn",
  "buildCommand": "yarn node scripts/build.js && mkdir -p public && touch public/index.html",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/(host|evcharger)",
      "destination": "api/index.js"
    }
  ],
  "github": {
    "enabled": false
  }
}
